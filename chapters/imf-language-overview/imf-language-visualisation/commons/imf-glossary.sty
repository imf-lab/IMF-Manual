\newglossaryentry{Artifact}{
    name={Artifact},
    description={Physical objects or software, with functions realized in activities, and situated in spatial locations}
}
\newglossaryentry{Aspect}{
    name={Aspect},
    description={An aspect is a particular way of viewing a facility asset. Different aspects have no overlap in information}
}
\newglossaryentry{Attribute}{
    name={Attribute},
    description={A quality or characteristic that someone or something has. Synonym with property}
}
\newglossaryentry{Capital Value Process}{
    name={Capital Value Process},
    description={Decision process for investment projects}
}
\newglossaryentry{Data engineer}{
    name={Data engineer},
    description={An expert on the flow of data between IT systems and applications}
}
\newglossaryentry{Design code}{
    name={Design code},
    description={Requirements to standardized engineering solution}
}
\newglossaryentry{Engineering register}{
    name={Engineering register},
    description={IT application holding attribute data}
}
\newglossaryentry{Facility asset}{
    name={Facility asset},
    description={An industrial plant, or part of it. A facility asset can be intended during design and actual when it has been installed}
}

\newglossaryentry{Facility asset information model}{
    name={Facility asset information model},
    description={A digital representation of a facility asset, also called an IMF model. IMF models can be integrated into a larger IMF model. In addition, IMF models may be extracted from a larger IMF model}
}
\newglossaryentry{Hierarchy}{
    name={Hierarchy},
    description={The hierarchy dimension stating elements being \emph{part of} higher-level elements, i.e., a breakdown}
}
\newglossaryentry{Purpose}{
    name={Purpose},
    description={When used in context of IMF types, a purpose states an intended activity that describes the essence of the IMF type}
}
\newglossaryentry{IMF type}{
    name={IMF type},
    description={A library resource used as starting point for instantiation of facility asset information model objects}
}
\newglossaryentry{IMF type library}{
    name={IMF type library},
    description={A library of IMF types that may be shared in the industry}
}
\newglossaryentry{Information model}{
    name={Information model},
    description={An information model is a structure of information objects and relations between them that is designed to capture knowledge}
}

\newglossaryentry{Knowledge engineer}{
    name={Knowledge engineer},
    description={An expert on knowledge representation including semantic technology}
}
\newglossaryentry{Media}{
    name={Media},
    description={The type of content of a stream, e.g., information, energy, material or force}
}
\newglossaryentry{Ontology}{
    name={Ontology},
    description={Technology that is used to represent knowledge in a formal way}
}
\newglossaryentry{Reference data library}{
    name={Reference data library},
    description={A managed collection of reference data~\cite{15926-1}, e.g., accessible through an endpoint. Reference data libraries provide industry shared resources, such as vocabularies, symbols, units}
}

\newglossaryentry{Reference designation}{
    name={Reference designation},
    description={Identifier of a specific object with respect to the system of which the object is a constituent, based on one or more aspects of that system~\cite{81346-1}}
}

\newglossaryentry{Stream}{
    name={Stream},
    description={The flow of media between systems}
}
\newglossaryentry{Subject matter expert}{
    name={Subject matter expert},
    description={An expert within a certain engineering discipline, for example a process or electrical engineer}
}
\newglossaryentry{System}{
    name={System},
    description={Combination of interacting elements organized to achieve one or more stated purpose~\cite{15288}}
}
\newglossaryentry{System engineer}{
    name={System engineer},
    description={An engineer focusing on how to design, integrate and manage complex system over their life cycles}
}
\newglossaryentry{TAG}{
    name={TAG},
    description={A human-readable code for identifying elements of a facility asset}
}
\newglossaryentry{Topology}{
    name={Topology},
    description={The topology dimensions stating how elements are \emph{connected to} each other}
}



